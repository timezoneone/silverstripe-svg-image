{
    "name": "wedevelopnl/silverstripe-svg-image",
    "description": "Silverstripe module to add support for SVG images.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "WeDevelop",
        "email": "development@wedevelop.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "enshrined/svg-sanitize": "^0.16",
        "meyfa/php-svg": "^0.14.5",
        "silverstripe/framework": "^5"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\SvgImage\\": "src/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "syntro/silverstripe-phpstan": "^5.0",
        "silverstripe/mimevalidator": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    },
    "suggest": {
        "silverstripe/mimevalidator": "Adds mimetype validation to file uploads."
    }
}
